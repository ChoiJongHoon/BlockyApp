<toolbox>

    <category name="이벤트" colour="45">
        <block type="event_start"></block>
    </category>

    <category name="데이터" colour="30" custom="VARIABLE">
        <block type="variables_get"></block>
        <block type="variables_set">
            <value name="VALUE">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="variables_change">
            <value name="DELTA">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="list_get"></block>
        <block type="list_add">
            <value name="TEXT">
                <shadow type="text">
                    <field name="TEXT">물건</field>
                </shadow>
            </value>
        </block>
        <block type="list_add_num">
            <value name="TEXT">
                <shadow type="text">
                    <field name="TEXT">물건</field>
                </shadow>
            </value>
        </block>
        <block type="list_delete_num"></block>
        <block type="list_change_num">
            <value name="TEXT">
                <shadow type="text">
                    <field name="TEXT">물건</field>
                </shadow>
            </value>
        </block>
        <block type="list_item"></block>
        <block type="list_length"></block>
        <block type="list_show"></block>
        <block type="list_hide"></block>
        <block type="list_include">
            <value name="TEXT">
                <shadow type="text">
                    <field name="TEXT">물건</field>
                </shadow>
            </value>
        </block>
    </category>

    <category name="로봇" colour="180">
        <!-- 카미봇 맵보드 -->
        <block type="robot_go">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="robot_turn_left"></block>
        <block type="robot_turn_right"></block>
        <block type="robot_turn_back"></block>

        <!-- 카미봇 -->
        <block type="robot_speed_front">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_speed_front_both">
            <value name="VALUE1">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
            <value name="VALUE2">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_speed_back">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_speed_back_both">
            <value name="VALUE1">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
            <value name="VALUE2">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_move_speed_left">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_move_speed_right">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_motor_left">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_motor_right">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_stop"></block>
        <block type="robot_set_colour"></block>
        <block type="robot_set_survmotor">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">30</field>
                </shadow>
            </value>
        </block>
        <block type="robot_sensor_ultrasonic"></block>
        <block type="robot_sensor_infrared"></block>
        <block type="robot_sensor_infrared_num"></block>
    </category>

    <category name="제어" colour="60">
        <block type="control_sleep">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="control_repeat_ext">
            <value name="TIMES">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="control_repeat_infinite"></block>
        <block type="control_if"></block>
        <block type="control_if_else"></block>
        <block type="control_sleep_until"></block>
        <block type="control_repeat_until"></block>
        <block type="control_stop"></block>
    </category>

    <category name="연산" colour="105">
        <block type="math_arithmetic">
            <value name="A">
                <shadow type="math_number"></shadow>
            </value>
            <value name="B">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="math_random">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="TO">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
        <block type="math_compare">
            <value name="A">
                <shadow type="math_number"></shadow>
            </value>
            <value name="B">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="math_operation">
            <value name="A">
                <shadow type="logic_boolean"></shadow>
            </value>
            <value name="B">
                <shadow type="logic_boolean"></shadow>
            </value>
        </block>
        <block type="math_negate">
            <value name="BOOL">
                <shadow type="logic_boolean"></shadow>
            </value>
        </block>
        <block type="math_modulo">
            <value name="DIVIDEND">
                <shadow type="math_number"></shadow>
            </value>
            <value name="DIVISOR">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="math_round">
            <value name="NUM">
                <shadow type="math_number"></shadow>
            </value>
        </block>
        <block type="math_formula">
            <value name="NUM">
                <shadow type="math_number"></shadow>
            </value>
        </block>
    </category>

    <category name="소리" colour="300">
        <block type="music_turnoff"></block>
        <block type="music_play_instrument">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0.25</field>
                </shadow>
            </value>
        </block>
        <block type="music_play_sound">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0.25</field>
                </shadow>
            </value>
        </block>
        <block type="music_sleep">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0.25</field>
                </shadow>
            </value>
        </block>
        <block type="music_choice_instrument"></block>
        <block type="music_change_sound">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">-10</field>
                </shadow>
            </value>
        </block>
        <block type="music_choice_sound">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
        <block type="music_change_tempo">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">20</field>
                </shadow>
            </value>
        </block>
        <block type="music_choice_tempo">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">60</field>
                </shadow>
            </value>
        </block>
        <block type="music_sound"></block>
        <block type="music_tempo"></block>
    </category>

</toolbox>
